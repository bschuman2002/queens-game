{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "github": {
    "silent": true
  },
  "projectSettings": {
    "framework": "nextjs",
    "buildCommand": "next build",
    "devCommand": "next dev",
    "outputDirectory": ".next"
  }
} 